#include <stdio.h>
#include <string.h>
 main()
{
	char hexa[8],bin[1000];
	int i=0,j=0;
	gets(hexa);
	for (i=0;i<8;i++)
	{
		switch(hexa[i])
		{
			case '0':
				bin[j++]='0';
				bin[j++]='0';
				bin[j++]='0';
				bin[j++]='0';
				break;
			case '1':
				bin[j++]='0';
				bin[j++]='0';
				bin[j++]='0';
				bin[j++]='1';
				break;
			case '2':
				bin[j++]='0';
				bin[j++]='0';
				bin[j++]='1';
				bin[j++]='0';
				break;
			case '3':
				bin[j++]='0';
				bin[j++]='0';
				bin[j++]='1';
				bin[j++]='1';
				break;
			case'4':
				bin[j++]='0';
				bin[j++]='1';
				bin[j++]='0';
				bin[j++]='0';
				break;
			case'5':
				bin[j++]='0';
				bin[j++]='1';
				bin[j++]='0';
				bin[j++]='1';
				break;
			case'6':
				bin[j++]='0';
				bin[j++]='1';
				bin[j++]='1';
				bin[j++]='0';
				break;
			case'7':
				bin[j++]='0';
				bin[j++]='1';
				bin[j++]='1';
				bin[j++]='1';
				break;
			case'8':
				bin[j++]='1';
				bin[j++]='0';
				bin[j++]='0';
				bin[j++]='0';
				break;
			case'9':
				bin[j++]='1';
				bin[j++]='0';
				bin[j++]='0';
				bin[j++]='1';
				break;
			case'A':
				bin[j++]='1';
				bin[j++]='0';
				bin[j++]='1';
				bin[j++]='0';
				break;
			case'B':
				bin[j++]='1';
				bin[j++]='0';
				bin[j++]='1';
				bin[j++]='1';
				break;
			case'C':
				bin[j++]='1';
				bin[j++]='1';
				bin[j++]='0';
				bin[j++]='0';
				break;
			case'D':
				bin[j++]='1';
				bin[j++]='1';
				bin[j++]='0';
				bin[j++]='1';
				break;
			case'E':
				bin[j++]='1';
				bin[j++]='1';
				bin[j++]='1';
				bin[j++]='0';
				break;
			case'F':
				bin[j++]='1';
				bin[j++]='1';
				bin[j++]='1';
				bin[j++]='1';
				break;
		}
	}
	puts(bin);
}
